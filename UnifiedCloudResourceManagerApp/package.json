{
  "name": "unified-cloud-resource-manager-app",
  "version": "0.1.0",
  "private": true,
  "description": "Unified Cloud Resource Manager - React app powered by Supabase",
  "_notes": [
    "Optional cloud provider SDKs are represented as placeholders below.",
    "Do NOT install these in the client; prefer secure serverless/edge integration gated by feature flags."
  ],
  "dependencies": {
    "@supabase/supabase-js": "^2.78.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "^5.0.1"
  },
  "optionalDependencies": {
    "@aws-sdk/client-ec2-placeholder": "0.0.0-PLACEHOLDER-DO-NOT-INSTALL",
    "@azure/arm-resources-placeholder": "0.0.0-PLACEHOLDER-DO-NOT-INSTALL",
    "@google-cloud/compute-placeholder": "0.0.0-PLACEHOLDER-DO-NOT-INSTALL",
    "aws-sdk-placeholder": "0.0.0-PLACEHOLDER-DO-NOT-INSTALL"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "REACT_APP_NODE_ENV=production react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint . --ext .js,.jsx"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "cypress": "^13.15.0"
  }
}
